2d_physics/layer_2="player_hands"
2d_physics/layer_3="fruits"
2d_physics/layer_4="pet"
2d_physics/layer_5="pet_area"
2d_physics/layer_6="enemy"
2d_physics/layer_7="enemy_hitbox"
2d_physics/layer_8="enemy_hurtbox"
2d_physics/layer_9="energy_blast"

[rendering]

quality/driver/driver_name="GLES2"
